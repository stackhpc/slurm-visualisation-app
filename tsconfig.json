{
    "compilerOptions": {
        "module": "system",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "rootDir": "src",
        "outDir": "dist",
        "sourceMap": true,
        "noUnusedLocals": false,
        "noImplicitReturns": false,
        "target": "es5",
        "lib": [
            "es2017",
            "es2017.object",
            "dom"
        ]
    },
    "filesGlob": [
        "node_modules/grafana-sdk-mocks/app/headers/common.d.ts",
        "src/**/*.ts"
    ],
    "files": [
        "node_modules/grafana-sdk-mocks/app/headers/common.d.ts",
        "src/components/config/config.ts",
        "src/components/monasca_client.ts",
        "src/globals.d.ts",
        "src/module.ts",
        "src/panels/jobView/job_view.ts",
        "src/panels/jobView/job_view_helpers.ts",
        "src/panels/jobView/module.ts",
        "src/panels/slurmView/canvas_manipulator.ts",
        "src/panels/slurmView/module.ts",
        "src/panels/slurmView/slurm_view.ts",
        "src/panels/slurmView/slurm_view_helpers.ts",
        "src/spec/job_view_specs.ts",
        "src/spec/slurm_view_specs.ts"
    ]
}
